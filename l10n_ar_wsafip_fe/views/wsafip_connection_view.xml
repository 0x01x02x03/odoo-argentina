<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_wsafip_auth_form" model="ir.ui.view">
            <field name="name">wsafip.auth.form</field>
            <field name="model">wsafip.connection</field>
            <field name="inherit_id" ref="l10n_ar_wsafip.view_wsafip_auth_form"/>
            <field name="arch" type="xml">
                <button name="do_login" position="after">
                    <button name="wsfe_get_status" string="Check Status" type="object" states="connected"/>
                    <button name="wsfe_update_afip_concept_type" string="Update Concept Type" type="object" states="connected"/>
                    <button name="wsfe_update_journal_class" string="Update Journal Class" type="object" states="connected"/>
                    <button name="wsfe_update_document_type" string="Update Document Type" type="object" states="connected"/>
                    <button name="wsfe_update_optional_types" string="Update Optional Types" type="object" states="connected"/>
                </button>
            </field>
        </record>
</data>
</openerp>